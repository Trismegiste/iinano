<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="abusive_listing" pattern="/abusereport/list">
        <default key="_controller">TrismegisteSocialBundle:Admin/AbuseReport:listing</default>
        <requirement key="_method">GET</requirement>
    </route>

    <route id="netizen_listing" pattern="/netizen/list">
        <default key="_controller">TrismegisteSocialBundle:Admin/Netizen:listing</default>
        <requirement key="_method">GET</requirement>
    </route>

</routes>